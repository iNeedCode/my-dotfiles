[github]
	user = ineedcode
[user]
  name = wonka
  email = XXXXXXX@YYYYY.COM
[mergetool]
	keepBackup = true  
[core]
  editor = vim
  excludesfile = /Users/wonka/.gitignore
[alias]
  co = checkout
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all  
  br = branch
  ci = commit  
  amend = commit --amend
  aa = add --all
  st = status
  pr = remote prune origin
  unstage = reset HEAD --
  last = log -1 -p HEAD
  visual = !gitk
  create = !sh -c 'git co -b \"$0\" && git push origin \"$0\" && git config \"branch.$0.remote\" origin && git config \"branch.$0.merge\" \"refs/heads/$0\"'
  purge = !sh -c 'git branch -d \"$0\" && git push origin :\"$0\"'
  fuckit = !git clean -d -f && git reset --hard
[color]
  ui = true
  [color "diff"]
    whitespace = red reverse  

[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

[format]
[rerere]
  enabled = 1
[push]
  default = current
[heroku]

  
